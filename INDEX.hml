<style>
   /* Reset some default styles */
body, h1, h2, p {
    margin: 0;
    padding: 0;
}

/* Styles for header */
header {
    background-color: #333;
    color: white;
    text-align: center;
    padding: 20px;
}


/* Styles for content */
.content {
  
    padding: 20px;
}

/* Styles for sections */
section {
    margin-bottom: 40px;
}

h2 {
    font-size: 24px;
    cursor: pointer;
}

/* Styles for dropdowns */
.dropdown {
    display: none;
    margin-top: 10px;
}

.dropdown.active {
    display: block;
}

.dropdown ul {
    list-style: none;
}

.dropdown ul li {
    margin: 5px 0;
}

/* Style for dropdown links */
.dropdown ul li a {
    text-decoration: none;
    color: #333;
    background-color: #f0f0f0;
    padding: 5px 10px;
    border-radius: 5px;
    display: inline-block;
    transition: background-color 0.3s ease;
}

/* Hover effect for dropdown links */
.dropdown ul li a:hover {
    background-color: #ddd;
}

/* Styles for nested dropdowns */
.nested-dropdown {
    display: none;
    margin-left: 20px;
    padding-left: 0;
}

.nested-dropdown.active {
    display: block;
}

.nested-dropdown li {
    margin: 5px 0;
}

/* Style for nested dropdown links */
.nested-dropdown li a {
    text-decoration: none;
    color: #333;
    background-color: #f0f0f0;
    padding: 5px 10px;
    border-radius: 5px;
    display: inline-block;
    transition: background-color 0.3s ease;
}

/* Hover effect for nested dropdown links */
.nested-dropdown li a:hover {
    background-color: #ddd;
}

</style>

<script>
document.querySelectorAll(".content section").forEach((section) => {
    const dropdown = section.querySelector(".dropdown");
    const header = section.querySelector("h2");

    if (dropdown) {
        header.addEventListener("click", () => {
            dropdown.classList.toggle("active");
        });
    }

    const nestedDropdown = section.querySelector(".nested-dropdown");
    if (nestedDropdown) {
        header.addEventListener("click", () => {
            nestedDropdown.classList.toggle("active");
        });
    }
});




</script>
